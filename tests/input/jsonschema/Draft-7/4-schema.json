{
  "$id": "not-a-uri",
  "$schema": "https://json-schema.org/draft-07/schema#",
  "name": "EmailCredential",
  "description": "$id is not a URI",
  "type": "object",
  "properties": {
    "credentialSubject": {
      "type": "object",
      "properties": {
        "emailAddress": {
          "type": "string",
          "format": "email"
        }
      },
      "required": [
        "emailAddress"
      ]
    }
  }
}
